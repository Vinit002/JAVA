package com.company;

import java.util.*;

public class Main {

    public static void main(String args[]){

        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        int pos = 2;

        int bitMask = 1<<pos;

        int notbitMask = ~ (bitMask);

        int newnumber = notbitMask & n ;

        System.out.println(newnumber);
    }
    }
